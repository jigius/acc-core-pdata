{
    "name": "jigius/acc-core-pdata",
    "description": "An yet another simple library for working with persistent storages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jigius",
            "email": "jigius@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "^7.4",
        "jigius/acc-core": "dev-master",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Acc\\Core\\PersistentData\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2@dev"
    },
    "repositories": [{
        "type": "vcs",
        "url": "git@github.com:jigius/acc-core.git"
    }],
    "suggest": {
        "jigius/acc-core-pdata-examples": "Shows trivial examples how to use this library"
    }
}
